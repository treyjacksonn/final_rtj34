@{
    ViewData["Title"] = "Add Entry";
}
<link href="~/lib/bootstrap/css/styles.css" rel="stylesheet" />
@model Entertainer 
<h1>Add Entry</h1>
<form method="post">

    <!--Provides validation of fields-->
    <div asp-validation-summary="All"></div>

    <label asp-for="EntStageName">Stage Name:</label>
    <input type="text" asp-for="EntStageName" /> <br />

    <label asp-for="EntSSN">Social Security:</label>
    <input type="number" asp-for="EntSSN" /> <br />

    <label asp-for="EntStreetAddress">Street Address:</label>
    <input type="text" asp-for="EntStreetAddress" /> <br />

    <label asp-for="EntCity">City:</label>
    <input type="text" asp-for="EntCity" /> <br />

    <label asp-for="EntState">State:</label>
    <input type="text" asp-for="EntState" /> <br />

    <label asp-for="EntZipCode">Zip Code:</label>
    <input type="number" asp-for="EntZipCode" /> <br />

    <label asp-for="EntPhoneNumber">Phone Number</label>
    <input type="text" asp-for="EntPhoneNumber" /> <br />

    <label asp-for="EntWebPage">Web Page:</label>
    <input type="text" asp-for="EntWebPage" /> <br />

    <label asp-for="EntEMailAddress">Email Address:</label>
    <input type="text" asp-for="EntEMailAddress" /> <br />

    <input type="submit" value="Add Entertainer" /> <br />

</form>